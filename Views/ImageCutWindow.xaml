<Window x:Class="LabelImg.ImageCutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabelImg"
        mc:Ignorable="d"
        Background="White"
        Title="ImageCutWindow" Height="300" Width="300">
    <Grid Background="{DynamicResource MainContentForegroundDrawingBrush}">
        <Image x:Name="imgCutDisplay" Stretch="Uniform"/>
    </Grid>
</Window>
